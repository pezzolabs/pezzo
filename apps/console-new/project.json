{
  "name": "console-new",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/console-new/src",
  "projectType": "application",
  "implicitDependencies": ["pezzo"],
  "targets": {
    "build": {
      "dependsOn": ["^graphql:generate", "^prebuild"],
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/console-new",
        "index": "apps/console-new/src/index.html",
        "baseHref": "/",
        "main": "apps/console-new/src/main.tsx",
        "tsConfig": "apps/console-new/tsconfig.app.json",
        "sourceMap": true,
        "postcssConfig": "apps/console-new/postcss.config.js",
        "assets": [
          "apps/console-new/src/favicon.ico",
          "apps/console-new/src/assets",
          {
            "glob": "nginx.conf",
            "input": "apps/console-new",
            "output": "."
          },
          {
            "glob": "**/*",
            "input": "apps/console-new/scripts",
            "output": "scripts"
          }
        ],
        "styles": [],
        "scripts": [],
        "isolatedConfig": false,
        "webpackConfig": "apps/console-new/webpack.config.js",
        "buildLibsFromSource": true
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/console-new/src/environments/environment.ts",
              "with": "apps/console-new/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "dependsOn": ["^graphql:generate"],
      "executor": "@nx/webpack:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "console-new:build",
        "hmr": true
      },
      "configurations": {
        "development": {
          "buildTarget": "console-new:build:development"
        },
        "production": {
          "buildTarget": "console-new:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/console-new/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "console-new:build"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/console-new/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "docker:build": {
      "executor": "@nx-tools/nx-container:build",
      "dependsOn": ["build"],
      "defaultConfiguration": "local",
      "options": {},
      "configurations": {
        "local": {
          "tags": ["pezzolabs/pezzo/console"],
          "push": false
        }
      }
    },
    "storybook": {
      "executor": "@nx/storybook:storybook",
      "options": {
        "port": 4400,
        "configDir": "apps/console-new/.storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@nx/storybook:build",
      "outputs": ["{options.outputDir}"],
      "options": {
        "outputDir": "dist/storybook/console",
        "configDir": "apps/console-new/.storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "test-storybook": {
      "executor": "nx:run-commands",
      "options": {
        "command": "test-storybook -c apps/console-new/.storybook --url=http://localhost:4400"
      }
    }
  },
  "tags": []
}
